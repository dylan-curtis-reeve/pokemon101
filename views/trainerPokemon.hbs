<div class="fPageCont">
    <div class="headingCont">
        <h1>Hi {{info.0.tName}}</h1>
    </div>
    <div class="pokemonContainer">
        {{#each info}}
            <div class="fPageCont">
                {{> pokCards}}
                <a href="/{{tId}}/swapPokemon/{{pId}}"><button >Swap Pokemon</button></a>
                <form action="/{{tId}}" method="post">
                    <input type="hidden" name="pId" value="{{pId}}">
                    <input type="submit" name="" value="Release Pokemon">
                </form>
            </div>
        {{/each}}
    </div>
    <div class="newCont">
        <a href="/{{info.0.tId}}/newPokemon"><button >Choose New Pokemon</button></a>
    </div>
    
</div>